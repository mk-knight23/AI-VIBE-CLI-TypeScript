{
    "name": "vibe-vscode",
    "displayName": "VIBE AI Teammate",
    "description": "VS Code integration for VIBE CLI",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "vibe.review",
                "title": "VIBE: Review Code"
            },
            {
                "command": "vibe.fix",
                "title": "VIBE: Fix Issue"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/node": "20.x",
        "@types/vscode": "^1.85.0",
        "@typescript-eslint/eslint-plugin": "^6.19.1",
        "@typescript-eslint/parser": "^6.19.1",
        "eslint": "^8.56.0",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "axios": "^1.13.5"
    }
}
